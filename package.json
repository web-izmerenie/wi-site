{
  "dependencies": {
    "LiveScript": "1.3.1",
    "body-parser": "^1.12.2",
    "bower": "^1.4.1",
    "co": "^4.5.1",
    "express": "^4.12.3",
    "front-end-gulp-pattern": "^1.0.2",
    "gulp-browserify": "~0.5.1",
    "gulp-stylus": "^1.3.4",
    "gulp-uglify": "^1.0.1",
    "jade": "^1.9.2",
    "js-yaml": "^3.2.7",
    "liveify-stable": "^1.0.0",
    "mz": "^1.3.0",
    "nib": "^1.0.4",
    "nodemailer": "^1.3.4",
    "prelude-ls": "1.1.1",
    "winston": "~1.0.0"
  },
  "scripts": {
    "postinstall": "./deploy.sh"
  },
  "gulp": {
    "distclean": [
      "./front-end-gulp",
      "./node_modules",
      "./bower_components",
      "./lsc"
    ],
    "scripts": {
      "site": {
        "type": "browserify",
        "path": "./scripts/",
        "srcDir": "./scripts/",
        "destDir": "./static/",
        "mainSrc": "main.ls",
        "buildFile": "build.js",
        "transform": [
          "liveify-stable"
        ],
        "extensions": [
          ".ls"
        ],
        "jshintDisabled": true,
        "shim": {
          "prelude": {
            "path": "./node_modules/prelude-ls/",
            "exports": ""
          },
          "jquery": {
            "path": "./bower_components/jquery/dist/jquery.min.js",
            "exports": ""
          },
          "jquery.transit": {
            "path": "./bower_components/jquery.transit/jquery.transit.js",
            "exports": ""
          },
          "modernizr": {
            "path": "./bower_components/modernizr/modernizr.js",
            "exports": "Modernizr"
          },
          "jquery.timers": {
            "path": "./scripts/lib/jquery.timers.js",
            "exports": "",
            "depends": {
              "jquery": "jQuery"
            }
          },
          "Snap.svg": {
            "path": "./bower_components/Snap.svg/dist/snap.svg-min.js",
            "exports": "Snap"
          },
          "get-val": {
            "path": "./js-useful-umd-modules/get-val/get-val.min.js",
            "exports": ""
          },
          "get-local-text": {
            "path": "./js-useful-umd-modules/get_local_text/get_local_text.js",
            "exports": ""
          },
          "load-img": {
            "path": "./js-useful-umd-modules/load_img/load_img.js",
            "exports": ""
          },
          "dynamic-api": {
            "path": "./js-useful-umd-modules/dynamic-api/dynamic-api.min.js",
            "exports": ""
          },
          "relative-number": {
            "path": "./js-useful-umd-modules/relative_number/relative_number.js",
            "exports": ""
          },
          "jquery-ui": {
            "path": "./bower_components/Smooth-Div-Scroll/js/jquery-ui-1.10.3.custom.min.js",
            "exports": "",
            "depends": {
              "jquery": "jQuery"
            }
          },
          "jquery-kinetic": {
            "path": "./bower_components/Smooth-Div-Scroll/js/jquery.kinetic.min.js",
            "exports": ""
          },
          "jquery-mousewheel-source": {
            "path": "./bower_components/Smooth-Div-Scroll/js/jquery.mousewheel.min.js",
            "exports": ""
          },
          "jquery-mousewheel": {
            "path": "./browserify-shim/jquery.mousewheel.js",
            "exports": ""
          },
          "gmaps": {
            "path": "./bower_components/gmaps/gmaps.min.js",
            "exports": "GMaps"
          },
          "smooth-div": {
            "path": "./bower_components/Smooth-Div-Scroll/js/jquery.smoothdivscroll-1.3-min.js",
            "exports": "",
            "depends": {
              "jquery": "jQuery",
              "jquery-ui": "",
              "jquery-mousewheel": "",
              "jquery-kinetic": ""
            }
          }
        }
      }
    },
    "styles": {
      "site": {
        "type": "stylus",
        "path": "./styles/",
        "srcDir": "./styles/",
        "destDir": "./static/",
        "mainSrc": "main.styl",
        "buildFile": "build.css",
        "shim": [
          "./stylus-shim/nib.js"
        ]
      }
    }
  }
}
